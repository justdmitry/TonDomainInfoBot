{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting": "Information",
      "System": "Warning"
    }
  },
  "AllowedHosts": "*",

  // Folder to store temporary tonlib data (sync info)
  "TonClientTempDir": ".",

  "TelegramOptions": {

    // Your bot token (from BotFather)
    "Token": "",

    // Public URI to your bot instance (to receive messages from Telegram)
    // Optional, if empty - bot will request new messages every 15sec.
    "WebhookUri": ""
  }
}
